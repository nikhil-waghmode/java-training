package patterns;
import java.util.Scanner;
/*
1
12
123
1234
12345 */

public class p1 {
	public static void main(String args[]) {
		System.out.println("Enter a number: ");
		Scanner sc = new Scanner(System.in);
		
		int n =sc.nextInt(), i, j;
		for (i =1;i<=n;i++) {
			for(j = 1;j<=i;j++) {
				System.out.print(j);
			}
			System.out.println();
		}
	}
}
