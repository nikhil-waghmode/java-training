package patterns;
import java.util.Scanner;
/*
    1
   222
  33333
 4444444
555555555
 */
public class p9 {
	public static void main(String args[]) {
		System.out.println("Enter a number: ");
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();	
		int i,j;
		for(i=1;i<=n;i++) {
			for(j=1;j<=n-1;j++) {
				System.out.print(" ");
			}
			System.out.println();
		}
		sc.close();
	}
}
