package day1;

import java.util.Scanner;

public class ass4 {
	public static void main(String arg[]) {
		Scanner obj = new Scanner(System.in);
		String name, address;
		name = obj.nextLine();
		address= obj.nextLine();
		
		System.out.println("Hello" + name + ". Please conform your delivery address: " + address);
		
		obj.close();
	}
}
